<Window x:Class="Cliente.PantUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cliente"
        mc:Ignorable="d"
        Title="PantUsuario" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Login" Width="100" Click="Login_Click"/>
            <Button Content="Registro" Width="100" Margin="15,0,0,0" Click="Registro_Click"/>
        </StackPanel>
        <Frame x:Name="main" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
